#include <stdio.h>
#include <stdlib.h>

void assert(int expected, int actual, char *code) {
    if (expected == actual) {
        printf("%s => %d\n", code, actual);
    } else {
        printf("%s => %d \033[31mexpected but got %d\033[0m\n", code, expected, actual);
        exit(1);
    }
}

// [75] 支持文件域内函数
static int static_fn() { return 5; }